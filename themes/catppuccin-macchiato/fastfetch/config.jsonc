{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/.config/fastfetch/ascii.ansi",
    "color": {
      "1": "#91d7e3",
      "2": "#8aadf4",
      "3": "#c6a0f6",
      "4": "#f5bde6",
      "5": "#f0c6c6"
    },
    "padding": {
      "top": 4,
      "right": 6
    }
  },
  "display": {
    "separator": "  "
